{
  "appId": "com.nayra.milkagencies",
  "productName": "Nayra Milk Agencies",
  "directories": {
    "output": "release/"
  },
  "files": [
    "dist/**/*",
    "electron/**/*"
  ],
  "win": {
    "target": [
      "nsis"
    ],
    "icon": "public/favicon.ico"
